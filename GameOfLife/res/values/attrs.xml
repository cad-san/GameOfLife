<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- name : custom view class name -->
    <declare-styleable name="LifeGridView">
        <attr name="numCellX" format="integer" />
        <attr name="numCellY" format="integer" />
        <attr name="lineWeight" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="cellColor1" format="color" />
        <attr name="cellColor2" format="color" />
        <attr name="cellColor3" format="color" />
        <attr name="cellColor4" format="color" />
        </declare-styleable>
</resources>
